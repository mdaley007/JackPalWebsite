{
    "presets": [
        "es2015",
        "react"
    ],
    "env": {
        "plugins": [
            "array-includes",
            "react-html-attrs",
            "syntax-class-properties",
            "syntax-decorators",
            "syntax-object-rest-spread",
            "transform-class-properties",
            "transform-decorators-legacy",
            "transform-object-assign",
            "transform-object-rest-spread"
        ]
    }
}
